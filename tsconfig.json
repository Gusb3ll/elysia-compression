{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "ESNext",
      "DOM",
      "ScriptHost"
    ],
    "module": "ES2022",
    "moduleResolution": "node",
    "types": [
      "bun-types"
    ],
    "declaration": true,
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  },
}